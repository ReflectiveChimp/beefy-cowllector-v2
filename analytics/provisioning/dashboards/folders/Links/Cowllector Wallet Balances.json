                    "editorMode": "code",
                    "format": "time_series",
                    "rawQuery": true,
                    "rawSql": "\nwith balance_ts as (\n  SELECT\n    chain,\n    date_trunc('hour', datetime) as datetime,\n    eth_wei_to_eth(min(balance_before_aggregated_wei)) as balance_before_total,\n    eth_wei_to_eth(min(balance_after_aggregated_wei)) as balance_after_total\n  FROM\n    cowllector_run \n  WHERE $__timeFilter(datetime) \n  GROUP BY 1,2, datetime\n),\nbalance_with_first_value_ts as (\n  SELECT\n    chain,\n      date_trunc('hour', datetime) as datetime,\n    balance_before_total,\n    FIRST_VALUE(balance_before_total) over (partition by chain order by datetime asc) as first_value_before,\n    balance_after_total,\n    FIRST_VALUE(balance_after_total) over (partition by chain order by datetime asc) as first_value_after\n  FROM\n    balance_ts\n),\naligned_ts as (\n  SELECT \n      chain,\n      date_trunc('hour', datetime) as datetime,\n    /*case when first_value_before != 0 \n      then balance_before_total / first_value_before\n      else 0\n    end as balance_before_percent,*/\n    case when first_value_after != 0 \n      then balance_after_total / first_value_after\n      else 0\n    end as balance_after_percent\n    from balance_with_first_value_ts\n)\n\nSELECT \n    chain,\n    datetime as time,\n    avg(balance_after_percent) as balance_after_percent\nfrom aligned_ts\nGROUP BY 1,2\nORDER BY\n  datetime",
                    "refId": "A",
                    "sql": {
                        "columns": [
                            {
                                "parameters": [],
                                "type": "function"
                            }
                        ],
